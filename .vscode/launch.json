{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "fixmatch-idx-3",
            "type": "debugpy",
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
                "CUDA_LAUNCH_BLOCKING": "1",
            },
            "request": "launch",
            "program": "fixmatch.py",
            "args": [
                "--project_name=ss2-ssl-idx-3",
                "--model_name=debug-fixmatch-2",
                // "--fast_debug",
                "--enable_logging",
                // "--enable_logging",

                "--dataset=idx_3",
                "--nclass=1",

                "--num_epochs=30",
                "--epochs_before_eval=5",
                "--save_path=exp/idx_3",
            ],
            "justMyCode": false,
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "fixmatch-idx-12",
            "type": "debugpy",
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            },
            "request": "launch",
            "program": "fixmatch.py",
            "args": [
                "--project_name=ss2-ssl-idx-12",
                "--model_name=debug-fixmatch-2",
                // "--fast_debug",
                "--enable_logging",
                // "--use_checkpoint",

                "--dataset=idx_12",
                "--nclass=3",

                "--num_epochs=10",
                "--epochs_before_eval=2",
                "--save_path=exp/idx_12",
            ],
            "justMyCode": false,
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
    ]
}